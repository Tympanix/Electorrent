{
  "name": "electorrent",
  "productName": "Electorrent",
  "version": "2.0.2",
  "description": "A thin client for your torrenting needs",
  "main": "app.js",
  "scripts": {
    "start": "electron --debug .",
    "build": "gulp build",
    "install": "bower install",
    "postinstall": "install-app-deps",
    "pack": "build --dir",
    "predist": "gulp build",
    "dist": "build",
    "dist:win": "build --win --x64",
    "dist:mac": "build --mac",
    "prerelease": "gulp build",
    "release": "build"
  },
  "keywords": [
    "utorrent"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Tympanix/Electorrent.git"
  },
  "author": "Tympanix",
  "dependencies": {
    "electron-regedit": "^1.0.5",
    "q": "^1.4.1",
    "winreg": "^1.2.2"
  },
  "devDependencies": {
    "bower": "^1.8.0",
    "electron": "^1.6.0",
    "electron-builder": "^17.0.0",
    "electron-builder-squirrel-windows": "^17.0.0",
    "electron-connect": "^0.6.1",
    "electron-is": "^2.4.0",
    "electron-squirrel-startup": "^1.0.0",
    "gulp": "^3.9.1",
    "gulp-clean": "^0.3.2",
    "gulp-run": "^1.7.1",
    "gulp-useref": "^3.1.0",
    "request": "^2.74.0",
    "run-sequence": "^1.2.2",
    "semver": "^5.3.0",
    "winston": "^2.2.0",
    "yargs": "^4.8.1"
  },
  "build": {
    "appId": "com.github.tympanix.electorrent",
    "mac": {
      "category": "public.app-category.utilities"
    },
    "win": {
      "target": "squirrel"
    },
    "extraFiles": [
      "resources"
    ]
  }
}
