{
  "name": "electorrent",
  "productName": "Electorrent",
  "version": "1.0.0",
  "description": "An Electron/Node/AngularJS remote client app for uTorrent server",
  "main": "app.js",
  "scripts": {
    "start": "electron .",
    "build": "gulp build",
    "install": "bower install",
    "postinstall": "install-app-deps",
    "pack": "build --dir",
    "dist:win": "build --win --x64",
    "dist:mac": "build --mac",
    "prerelease": "gulp build",
    "release": "build"
  },
  "keywords": [
    "utorrent"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Tympanix/Electorrent.git"
  },
  "author": "tympanix",
  "devDependencies": {
    "electron-prebuilt": "^1.2.6",
    "electron-squirrel-startup": "^1.0.0"
  },
  "dependencies": {
    "bower": "^1.7.9",
    "electron-builder": "^5.11.3",
    "electron-connect": "^0.4.2",
    "gulp": "^3.9.1",
    "gulp-clean": "^0.3.2",
    "gulp-run": "^1.7.1",
    "gulp-useref": "^3.1.0",
    "run-sequence": "^1.2.1",
    "winston": "^2.2.0"
  },
  "build": {
    "appId": "dk.sparken.electorrent",
    "app-category-type": "public.app-category.utilities"
  }
}
